programa {
  funcao inicio() {
    real conta, resulFinal 
    inteiro quantPessoa
    escreva ("Digite o valor total da conta:")
    leia (conta)
    escreva ("Digite a quantidade de pessoas:")
    leia (quantPessoa)
    resulFinal = conta / quantPessoa
    escreva ("Cada um devera pagar:", resulFinal)
  }
}
